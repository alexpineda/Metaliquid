<?xml version="1.0" encoding="UTF-8" ?>
<!-- These are injected into the other templates at the xml designers will  -->
<!-- TODO: change item to iitem -->
<templates>

<forum >

	<group>threads</group>
	
	<pagination>
		<!--url>ampcurrentpage=PAGE_NUM</url-->
	</pagination>

	<columns_diagram startatrow="2">
		<column_diagram id="topic" title="Topic">//table[@class='solid']/tr[%ROW%]/td[2]/a[1]</column_diagram>
		
		<column_diagram id="topichref" title="Topic Link" filter="href">//table[@class='solid']/tbody/tr[%ROW%]/td[2]/a[1]</column_diagram>
		
		<column_diagram id="topicstarter" title="Topic Starter" >//table[@class='solid']/tbody/tr[%ROW%]/td[3]</column_diagram>
		
		<column_diagram id="replies" title="Replies" >//table[@class='solid']/tbody/tr[%ROW%]/td[4]</column_diagram>
		
		<column_diagram id="views" title="Views" >//table[@class='solid']/tbody/tr[%ROW%]/td[5]</column_diagram>
		
		<column_diagram id="lastreply" title="Last Reply" >//table[@class='solid']/tbody/tr[%ROW%]/td[6]</column_diagram>
	</columns_diagram>
	
</forum>





<pelo >

	<group>pro</group>
	<subgroup>pelo</subgroup>
	
	<columns_diagram startatrow="2" >
	<column_diagram id="race" title="Race" filter="title">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[1]/img</instruction>
	</column_diagram>

	<column_diagram id="name" title="Player Name" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[1]/a</instruction>
	</column_diagram>
	
	<column_diagram id="namehref" title="Player Name Link"  filter="href">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[1]/a</instruction>
	</column_diagram>

	<column_diagram id="team" title="Team" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[2]/a</instruction>
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[2]</instruction>
	</column_diagram>
	
	<column_diagram id="teamhref" title="Team Link" filter="href" optional="true">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[2]/a</instruction>
	</column_diagram>
		
	<column_diagram id="peak_overall" title="Peak Overall" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[4]/a</instruction>
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[4]</instruction>
	</column_diagram>
	
	<column_diagram id="peak_overallhref" title="Peak Overall Link" filter="href" optional="true">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[4]/a</instruction>
	</column_diagram>
		
	<column_diagram id="peak_vs_terran" title="PeakVsT" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[6]/a</instruction>
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[6]</instruction>
	</column_diagram>
	
	<column_diagram id="peak_vs_terranhref" title="PeakVsT Link" filter="href" optional="true">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[6]/a</instruction>
	</column_diagram>
		
	<column_diagram id="peak_vs_zerg" title="PeakVsZ" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[8]/a</instruction>
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[8]</instruction>
	</column_diagram>
	
	<column_diagram id="peak_vs_zerghref" title="PeakVsZ Link" filter="href" optional="true">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[8]/a</instruction>
	</column_diagram>
		
	<column_diagram id="peak_vs_protoss" title="PeakVsP" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[10]/a</instruction>
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[10]</instruction>
	</column_diagram>
	
	<column_diagram id="peak_vs_protosshref" title="PeakVsP Link" filter="href" optional="true">
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[10]/a</instruction>
	</column_diagram>
	
	<column_diagram id="current_overall" title="Current Overall" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[3]</instruction>
	</column_diagram>

	<column_diagram id="current_vs_terran" title="CurrentVsT" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[5]/span</instruction>
	</column_diagram>
			
	<column_diagram id="current_vs_zerg" title="CurrentVsZ" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[7]/span</instruction>
	</column_diagram>

	<column_diagram id="current_vs_protoss" title="CurrentVsP" >
		<instruction>//table[@id='tblt_table']/tr[%ROW%]/td[9]/span</instruction>
	</column_diagram>
</columns_diagram>

</pelo>
</templates>